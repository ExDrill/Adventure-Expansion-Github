			"cannot_walk": {
				"remove": {
					"component_groups": [
						"can_move"
					]
				}
			},
			"flight": {
				"add": {
					"component_groups": [
						"is_ignited"
					]
				}
			},
			"grounded": {
				"remove": {
					"component_groups": [
						"is_ignited"
					]
				}
			},
			"on_eat_item": {
				"randomize": [
					{
						"add": {
							"component_groups": [
								"allows_riding"
							]
						},
						"remove": {
							"component_groups": [
								"is_hungry"
							]
						},
						"run_command": {
							"command": [
								"particle minecraft:villager_happy ~ ~2 ~"
							]
						},
						"weight": 1
					},
					{
						"run_command": {
							"command": [
								"particle minecraft:basic_smoke_particle ~ ~2 ~"
							]
						},
						"weight": 2
					}
				]
